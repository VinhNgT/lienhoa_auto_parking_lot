meta {
  name: Set new status light state
  type: http
  seq: 4
}

patch {
  url: http://{{hostname}}/status_lights_state/
  body: formUrlEncoded
  auth: none
}

headers {
  Content-Type: application/x-www-form-urlencoded
  User-Agent: insomnia/10.0.0
}

body:form-urlencoded {
  state: none
  ~state: ready
  ~state: processing
  ~state: allow
  ~state: deny
}
